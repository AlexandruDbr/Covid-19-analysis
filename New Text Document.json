{
    "name": "Custom Font Sizes Theme",
    "dataColors": [
        "#01B8AA",
        "#374649",
        "#FD625E",
        "#F2C80F",
        "#5F6B6D",
        "#8AD4EB",
        "#FE9666",
        "#A66999",
        "#3599B8",
        "#DFBFBF"
    ],
    "visualStyles": {
        "*": {
            "*": {
                "general": [
                    {
                        "responsive": true
                    }
                ]
            },
            "textbox": {
                "*": {
                    "title": [
                        {
                            "fontSize": 130
                        }
                    ],
                    "paragraphs": [
                        {
                            "fontSize": 130
                        }
                    ]
                }
            },
            "card": {
                "*": {
                    "categoryLabels": [
                        {
                            "fontSize": 80
                        }
                    ],
                    "labels": [
                        {
                            "fontSize": 80
                        }
                    ]
                }
            }
        }
    }
}
